(page "index.html"
      (:require
        [auth.screen]

        [devtools.core :as devtools]))

(with-init!
  (devtools/install!))

(html
  (head
    (html-meta :name "viewport"
          :content (str "width=device-width, "
                        "initial-scale=1.0, "
                        "maximum-scale=1.0, "
                        "user-scalable=no"))
)
  (body
    (main (cell= (auth.screen/content)))))
